{
  "parser": "babel-eslint",
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier", "react"],
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "max-nested-callbacks": ["warn", 3],
    "react/jsx-uses-vars": "error"
  }
}
